//스레드 테스트용 패키지
package thread_test;

public class ThreadMain {

	public static void main(String[] args) {
		Thread t1 = new Thread(new Data(10)); //쓰레드 객체 생성
		t1.start(); //쓰레드 시작
		System.out.println("쓰레드 생성됨.");
		
		for(int i = 0; i <= 10; i++) {
			System.out.println("Main 다른 작업중:" + i);
		}
		
		System.out.println("Main 끝남");
		
	}

}
