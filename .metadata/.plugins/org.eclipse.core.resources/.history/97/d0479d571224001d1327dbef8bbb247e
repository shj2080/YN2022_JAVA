/** [Stack 클래스]
 * Stack : 맨 나중에 추가된 데이터를 먼저 꺼내는 방식(LIFO : Last In First Out)
 * 
 * 가장 최근에 추가된 자료부터 반환해주므로
 * 가장 최근에 검색한 단어를 찾는다던가,
 * 장기 체스 같은 게임에서 수를 무를 때 응용할 수 있다.
 * 
 */
package stack;

import java.util.ArrayList;

//ArrayList로 Stack 구현하기
class MyStack {
	//멤버변수 : ArrayList 포함관계
	//[포함관계 순서-1]
	private ArrayList<String> arrayStack;

	MyStack() { //[포함관계 순서-2]
		arrayStack = new ArrayList<String>();
	}
	
	//[포함관계 순서-3]
	//외부에 노출된 메서드
	void push(String data) {
		arrayStack.add(data); //실질적인 메서드
	}
	
	String push() { 
		int number = arrayStack.size();
		
		if(number == 0) {
			System.out.println("스택이 비었습니다.");
			return null;
		}
		
		return arrayStack.remove(number-1); //전체 size -1 = ArrayList의 마지막 index
	}
	
}//MyStack 클래스 끝

public class StackTest_ArrayList {

	public static void main(String[] args) {
		
		
	}

}
