package forwhiledowhile;

public class ForWhileDowhile {

	public static void main(String[] args) {
		/** 반복문 사용 안하면 **/
		System.out.print(1+ " ");
		System.out.print(2+ " ");
		System.out.print(3+ " ");
		System.out.print(4+ " ");
		System.out.print(5+ " ");
		
		System.out.println(); //구분용 빈줄
		
		/** 반복문 3가지 **/
		System.out.println("*** for문 : \"배열\"이나 \"컬렉션프레임워크\"에서 많이 사용함 ***");
		
		//1~5까지 옆으로 출력 for문
		for(int i = 1; i <= 5; i++) {
			System.out.print(i + " ");
		}
	}

}
