package cal;

public class CalculatorMain {

	public static void main(String[] args) {
		/* 잘못된 방법
		//각 부품 생성
		Add a = new Add();
		Sub s = new Sub();
		Mul m = new Mul();
		Div d = new Div();
		
		new Calculator(a, s, m, d);
		*/
		
		Calculator cal = new Calculator();
		System.out.println("10+20=" + cal.add(10, 20));
	}

}
