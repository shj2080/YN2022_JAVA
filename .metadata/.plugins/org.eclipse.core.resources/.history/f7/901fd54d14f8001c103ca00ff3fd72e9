
public class ArrayTesting {

	public static void main(String[] args) {
		int i;
		
		System.out.println("◈과제-Arrays.toString 없이 [A, B, C] 출력"); char[] ch2 = {'A',
				  'B', 'C'};
				  
				  System.out.print("["); for(i = 0;i < ch2.length; i++) { if(i == ch2.length-1)
				  { //ch2의 마지막 index값인 경우(length-1) System.out.print(ch2[i]); //, 제외하여 출력 }else
				  { System.out.print(ch2[i] + ", "); //, 포함하여 출력 } } System.out.print("]");
				  System.out.println(); //커서를 아래로 내림
				 //-----------------------------------------------------------------------	
		
	}	


}
